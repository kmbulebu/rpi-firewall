SUBSYSTEM=="net", ACTION=="add", KERNEL=="{{ firewall_wan_iface }}" RUN{program}+="/bin/bash -c 'for x in /sys/class/net/$name/queues/rx-*; do echo 1 > $$x/rps_cpus; done'"
SUBSYSTEM=="net", ACTION=="add", KERNEL=="{{ firewall_lan_iface }}" RUN{program}+="/bin/bash -c 'for x in /sys/class/net/$name/queues/rx-*; do echo 2 > $$x/rps_cpus; done'"
