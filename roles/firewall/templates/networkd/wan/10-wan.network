[Match]
Name={{ wan_iface }}

[Link]
RequiredForOnline=no

[Network]
LinkLocalAddressing=ipv6
DHCP=yes
IPv6AcceptRA=yes
ConfigureWithoutCarrier=no
IPv6PrivacyExtensions=false
LLMNR=false
MulticastDNS=false

[IPv6AcceptRA]
UseDNS=false

[DHCP]
UseMTU=true

[DHCPv4]
UseDNS=false
ClientIdentifier=mac
SendHostname=false
UseHostname=false

[DHCPv6]
UseDNS=false
PrefixDelegationHint=::/60

[Route]
Gateway=_ipv6ra
IPv6Preference=high
